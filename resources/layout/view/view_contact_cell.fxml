<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Circle?>
<VBox xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.sayit.ui.control.view.ContactViewController">
    <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
          minHeight="-Infinity" minWidth="-Infinity" prefHeight="55.0" styleClass="root-container"
          VBox.vgrow="ALWAYS">
        <Circle fx:id="roundedImage" fill="DODGERBLUE" radius="23.0" stroke="BLACK" strokeType="INSIDE"
                strokeWidth="0.0"/>
        <VBox alignment="CENTER_LEFT" styleClass="label-container" HBox.hgrow="ALWAYS">
            <Label fx:id="nameLabel" styleClass="name-label" text="Label"/>
            <Label fx:id="descLabel" maxWidth="100.0" styleClass="desc-label"
                   text="Adamastor caiano de sousa"/>
        </VBox>
        <HBox alignment="TOP_RIGHT" styleClass="time-container" HBox.hgrow="SOMETIMES">
            <Label fx:id="timeLabel" styleClass="desc-label" text="Label"/>
        </HBox>
    </HBox>
    <HBox maxHeight="2.0" minHeight="1.0" styleClass="line-divisor">
        <VBox.margin>
            <Insets left="66.0" right="10.0"/>
        </VBox.margin>
    </HBox>
</VBox>
